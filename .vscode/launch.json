{ "version": "0.2.0"
, "configurations"
  : [   { "name": "Flash"
        , "type": "fdb"
        , "request": "launch"
        , "preLaunchTask": "flash debug (build only)"
        , "program": "${workspaceRoot}/bin/flash/bin/RapellaFella.swf"
        }
    ,   { "name"      : "Html5"
        , "type"      : "chrome"
        , "request"   : "launch"
        , "url"       : "http://127.0.0.1:3000"
        , "sourceMaps": true
        , "webRoot"   : "${workspaceFolder}"
        }
    ]
}